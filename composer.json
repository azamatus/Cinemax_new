{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",

    "autoload": {
        "psr-0": { "": "src/" }
    },

    "require": {
        "php": ">=5.3.3",

        "twig/twig": "1.12.*",
        "twig/extensions": "1.0.*",

        "symfony/symfony": "2.3.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",

        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",

        "jms/security-extra-bundle": "~1.5",
        "jms/di-extra-bundle": "~1.4",

        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "doctrine/data-fixtures": "1.0.*@dev",

        "knplabs/gaufrette": "0.1.*",

        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/seo-bundle": "~1",
        "sonata-project/formatter-bundle": "~2.3",
        "sonata-project/doctrine-extensions": "~1",
        "sonata-project/exporter": "~1.3",
        "sonata-project/notification-bundle": "~2.2",
        "sonata-project/admin-bundle": "~2.2",
        "sonata-project/block-bundle": "~2.2",
        "sonata-project/cache-bundle": "~2.1",
        "sonata-project/user-bundle": "~2.2",
        "sonata-project/media-bundle": "~2.2",
        "sonata-project/news-bundle": "~2.3",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/core-bundle": "~2.2@dev",

        "symfony-cmf/routing": "~1.1",

        "friendsofsymfony/user-bundle": "1.3.*",

        "dflydev/markdown": "1.0.2",
        "simplethings/entity-audit-bundle": "dev-master",
        "willdurand/faker-bundle": "dev-master",

        "kriswallsmith/assetic": "1.1.*@dev",
        "friendsofsymfony/jsrouting-bundle": "~1.1",
        "knplabs/knp-paginator-bundle": "dev-master",
        "gregwar/captcha-bundle": "dev-master",
        "a2lix/translation-form-bundle": "1.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },

    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
