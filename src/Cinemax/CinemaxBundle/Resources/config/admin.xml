<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sonata.admin.discs" class="Cinemax\CinemaxBundle\Admin\ContentAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Диски" label="Диски"/>
            <argument />
            <argument>Cinemax\CinemaxBundle\Entity\Discs</argument>
            <argument>SonataAdminBundle:CRUD</argument>
            <call method="setTranslationDomain">
                <argument>SonataAdminBundle</argument>
            </call>
        </service>
        <service id="sonata.admin.countries" class="Cinemax\CinemaxBundle\Admin\CountriesAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Диски" label="Страны"/>
            <argument />
            <argument>Cinemax\CinemaxBundle\Entity\Countries</argument>
            <argument>SonataAdminBundle:CRUD</argument>
            <call method="setTranslationDomain">
                <argument>SonataAdminBundle</argument>
            </call>
        </service>
        <service id="sonata.admin.janrs" class="Cinemax\CinemaxBundle\Admin\JanrsAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Диски" label="Жанры"/>
            <argument />
            <argument>Cinemax\CinemaxBundle\Entity\Janrs</argument>
            <argument>SonataAdminBundle:CRUD</argument>
            <call method="setTranslationDomain">
                <argument>SonataAdminBundle</argument>
            </call>
        </service>
        <service id="sonata.admin.types" class="Cinemax\CinemaxBundle\Admin\TypesAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Диски" label="Типы"/>
            <argument />
            <argument>Cinemax\CinemaxBundle\Entity\Types</argument>
            <argument>SonataAdminBundle:CRUD</argument>
            <call method="setTranslationDomain">
                <argument>SonataAdminBundle</argument>
            </call>
        </service>
    </services>
</container>